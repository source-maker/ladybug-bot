{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Test PR Health Check",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/.github/scripts/health-check.py",
            "env": {
                "GITHUB_TOKEN": "your_github_token_here",
                "GITHUB_EVENT_PATH": "${workspaceFolder}/.vscode/pull_request_event.json",
                "GITHUB_REPOSITORY": "your_username/your_repository"
            },
            "console": "integratedTerminal",
        }
    ]
}
